{
	"name": "sugar-keyboard",
	"version": "0.0.77",
	"scripts": {
		"serve": "vue-cli-service serve --mode dev",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"prepare-All": "vue-cli-service build --target lib --name sugar-ui ./src/plugs/index.js --description sugar-ui --keywords sugar-ui --author sugar_free",
		"prepare-keyBoard": "vue-cli-service build --target lib --name keyboard ./src/plugs/keyboard/index.js --description keyboard --keywords keyboard --author sugar_free"
	},
	"dependencies": {
		"core-js": "^3.6.5",
		"element-ui": "^2.13.2",
		"svg-sprite-loader": "^5.0.0",
		"url-loader": "^4.1.0",
		"vue": "^2.6.11"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~4.5.0",
		"@vue/cli-plugin-eslint": "~4.5.0",
		"@vue/cli-service": "~4.5.0",
		"babel-eslint": "^10.1.0",
		"eslint": "^6.7.2",
		"eslint-plugin-vue": "^6.2.2",
		"vue-template-compiler": "^2.6.11"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"rules": {
			"no-debugger": 0
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	],
	"main": "./dist/keyboard.common.js",
	"description": "keyboard",
	"keywords": [
		"keyboard"
	],
	"author": "sugar_free"
}